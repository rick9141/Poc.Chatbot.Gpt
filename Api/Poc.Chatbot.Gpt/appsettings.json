{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Settings": {
    "AuthenticationSettings": {
      "ApiKey": ""
    },
    "DefaultTimeoutMs": 6000,
    "DelayBetweenErrors": 1000,
    "RequestMaxRetries": 3
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "",
          "apiKey": ""
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
